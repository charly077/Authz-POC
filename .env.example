# AI Manager OIDC (must match AIManagerRealm client secret)
AI_MANAGER_CLIENT_SECRET=ai-manager-secret
AI_MANAGER_ADMIN_PASSWORD=admin
SESSION_SECRET=change-me-to-a-random-string
